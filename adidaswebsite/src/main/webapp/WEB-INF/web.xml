<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>adidaswebsite</display-name>
  <welcome-file-list><welcome-file>login.jsp</welcome-file></welcome-file-list>
  <servlet>
  	<servlet-name>servlet1</servlet-name>
  	<servlet-class>com.valid.Login</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servlet1</servlet-name>
  	<url-pattern>/login</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>servlet2</servlet-name>
  	<servlet-class>com.valid.logout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servlet2</servlet-name>
  	<url-pattern>/logout</url-pattern>
  </servlet-mapping>
  
  <servlet>
  	<servlet-name>servlet3</servlet-name>
  	<servlet-class>com.valid.checkout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servlet3</servlet-name>
  	<url-pattern>/checkout</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>servlet4</servlet-name>
  	<servlet-class>com.valid.clearSession</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servlet4</servlet-name>
  	<url-pattern>/clearSession</url-pattern>
  </servlet-mapping>
  
   <servlet>
  	<servlet-name>servlet5</servlet-name>
  	<servlet-class>com.valid.Register</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>servlet5</servlet-name>
  	<url-pattern>/Register</url-pattern>
  </servlet-mapping>
</web-app>